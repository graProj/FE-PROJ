    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/themes": "^3.1.1",
    "@tanstack/query-sync-storage-persister": "^5.35.1",
    "@tanstack/react-query": "^5.26.3",
    "@tanstack/react-query-persist-client": "^5.35.1",
    "axios": "^1.6.8",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "electron-alert": "^0.1.20",
    "electron-is-dev": "^3.0.1",
    "eslint": "^8.57.0",
    "framer-motion": "^11.3.20",
    "lucide-react": "^0.418.0",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "socket.io-client": "^4.3.1",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "^3.2.7",
    "tailwindcss-animate": "^1.0.7",
    "web-vitals": "^2.1.4",
    "zustand": "^4.5.2"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "concurrently \"react-scripts start\" \"npm run electron\" ",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "wait-on http://localhost:3000 && electron .",
    "pack": "npm run build && electron-builder build -c.extraMetadata.main=build/main.js"
  },
  "build": {
    "files": [
      "build/**/*"
    ],
    "icon": "build/logo.png",
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "^10.4.7",
    "electron": "^29.1.4",
    "electron-builder": "^24.13.3",
    "postcss": "^8.4.16",
    "wait-on": "^7.2.0"
  }
}
